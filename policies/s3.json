{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Action": [
                "s3:GetBucketLocation",
                "s3:GetObject",
                "s3:ListBucket",
                "s3:ListBucketMultipartUploads",
                "s3:ListMultipartUploadParts",
                "s3:AbortMultipartUpload",
                "s3:PutObject"
            ],
            "Resource": [
                "arn:aws:s3:::billing-cloud-carbon-footprint",
                "arn:aws:s3:::billing-cloud-carbon-footprint/*",
                "arn:aws:s3:::query-cloud-carbon-footprint",
                "arn:aws:s3:::query-cloud-carbon-footprint/*"
            ],
            "Effect": "Allow"
        }
    ]
}